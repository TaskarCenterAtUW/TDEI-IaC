{
    "server_name" : "tdei-postgresql",
    "secrets" : {
        "administrator_login" : "{postgresql-administrator-login}",
        "administrator_login_password" : "{postgresql-administrator-password}"
    },
    "sku": {
        "name": "Standard_B2s",
        "tier": "Burstable"
    },
    "availabilityZone": "1",
    "backup": {"backupRetentionDays": 7, "geoRedundantBackup": "Disabled"},
    "highAvailability": {"mode": "ZoneRedundant"},
    "storage": {"autoGrow": "Disabled", "storageSizeGB": 32, "tier": "P10"},
    "version" : 14,
    "create_mode" : "Create",
    "database" : [
        "tdei"
    ]
}
