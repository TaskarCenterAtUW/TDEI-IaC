{
    "server_name" : "tdei-postgresql",
    "secrets" : {
        "administrator_login" : "{postgresql-administrator-login}",
        "administrator_login_password" : "{postgresql-administrator-password}"
    },
    "sku": {
        "name": "Standard_B4ms",
        "tier": "Burstable"
    },
    "availabilityZone": "1",
    "backup": {"backupRetentionDays": 30, "geoRedundantBackup": "Enabled"},
    "highAvailability": {"mode": "ZoneRedundant"},
    "version" : 16,
    "storage": {"autoGrow": "Enabled", "storageSizeGB": 32, "tier": "P15"},
    "create_mode" : "Create",
    "database" : [
        "tdei"
    ]
}
